{
  "name": "gg-website",
  "private": true,
  "description": "Guillaume Grassiant - Web Developer from Montreal",
  "version": "0.1.0",
  "author": "Guillaume Grassiant <guillaumegrassiant@hey.com>",
  "keywords": [
    "gatsby",
    "React",
    "TypeScript",
    "Styled Components",
    "Portfolio",
    "Software Developer",
    "Canada",
    "Montreal",
    "Contenful"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false --updateSnapshot",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "check-before-commit": "npm run lint && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GGrassiant/gg-website"
  },
  "bugs": {
    "url": "https://github.com/GGrassiant/gg-website"
  },
  "dependencies": {
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "gatsby": "^2.30.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^2.6.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.8.0",
    "gatsby-source-contentful": "^4.4.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-theme-i18n": "^1.0.5",
    "gatsby-theme-i18n-react-intl": "^1.0.4",
    "gatsby-transformer-sharp": "^2.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-intl": "^4.6.9",
    "styled-components": "^5.2.1",
    "swr": "^0.4.0",
    "terser": "^3.16.1 "
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.10.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "react-test-renderer": "^16.14.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
